package domain;

import domain.value.Text;
import domain.value.id.ThemeID;

public class Theme {

	private ThemeID themeID;	// テーマID
	private Text text =new Text("");			// テーマ

	public Theme()
	{}

	public Theme(ThemeID themeID, Text text) {
		super();
		this.themeID = themeID;
		this.text = text;
	}


	public Theme(ThemeID themeID) {
		super();
		this.themeID = themeID;
	}

	public ThemeID getThemeID() {
		return themeID;
	}

	public void setThemeID(ThemeID themeID) {
		this.themeID = themeID;
	}

	public Text getText() {
		return text;
	}

	public void setText(Text text) {
		this.text = text;
	}



}
